{
  "name": "fast-tcp",
  "version": "3.3.2",
  "description": "fast-tcp is an extremely fast TCP client and server that allows to emit and listen to events",
  "author": "Alejandro Santiago Nieto",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.1.0",
    "istanbul": "^0.4.5",
    "utjs": "^2.1.0"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "eslint": "^4.15.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.0"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "test": "mocha test/unit/mocha.conf.js test/unit/*.js",
    "test:cover": "istanbul cover --report cobertura _mocha test/unit/mocha.conf.js test/unit/*.js",
    "benchmark": "node test/benchmark/*.js",
    "lint": "eslint index.js lib/*.js test/**/*.js",
    "doc": "jsdoc ./lib/ ./package.json ./README.md -d ./doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alemures/fast-tcp.git"
  }
}
